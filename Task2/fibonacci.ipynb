   ],
   "source": [
    "k=1\n",
    "n=2\n",
    "i=3\n",
    "while i<100:\n",
    "    g=n+k #the value of the fibonacci term\n",
    "    k=n #setting k as the second previous value\n",
    "    n=g #setting n as the previous value\n",
    "    i=i+1 #adding a number of terms\n",
    "\n",
    "print(f\"The 100th Fibonnacci term has a value of {g}\")"
   ]
